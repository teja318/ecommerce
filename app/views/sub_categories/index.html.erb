<h2>Listing SubCategories</h2>
<% if @sub_categories.empty? %>

  <p> No products found </p>
<% else %>	
<ul>
<% @sub_categories.each do |sub_category| %>
<li><%=  sub_category.name %>
   <%#sub_category.category.name %>
   <%= link_to "show", sub_category_path(sub_category.id) %>
   </li>
   <%end%>
   </ul>
   <%end%>
   <%#= link_to sub_categories_path %>