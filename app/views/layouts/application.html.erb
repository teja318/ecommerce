<!DOCTYPE html>
<html>
<head>
  <title>Ecommerce</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
   <%= javascript_include_tag "https://www.gstatic.com/charts/loader.js" %>

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= link_to "Products", products_path %> | <%= link_to "Categories", categories_path %>
<% if user_signed_in? %>
<%= link_to "Cart", cart_line_items_path %> | <%= link_to "Order", orders_path %> 
<%= link_to "Address", addresses_path %>

<%= current_user.username %>

<%= link_to "Dashboard", dashboard_home_path %>
<%= link_to "AccountSetting", edit_user_registration_path %>
<%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% else %> 
<%= link_to "Login", new_user_session_path %>
<%= link_to "Register", new_user_registration_path %>
<% end %>

<%= link_to "Wishlists", wish_lists_path %>
 <p class="notice"><%= notice %></p>
 <p class="alert"><%= alert %></p>

<%= yield %>

</body>
</html>
